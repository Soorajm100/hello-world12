<body>
   
    
   
        
    <div class="container">
        <div id="topframe"><h1 style="text-align: center; color:cyan">BLOG SPOT</h1></div>
        <span class="menu-icon" style="font-size:35px;cursor:pointe;"  onclick="openNav()">&#9776;</span>
      <div class="wrap">
    <div class="search">
       <input type="text" class="searchTerm" placeholder="Search ....">
       <button type="submit" class="searchButton">
         <i class="fa fa-search"></i>
      </button>
    </div>
 </div>
 <div class="menu-container" id="myNav">
    <a href="javascript:void(0)" class="closebtn"
    onclick="closemenu()">&times;
</a> 
    <div class="menu-content">
        <a href="#">HOME</a>
        <a href="#">ABOUT</a>
        <a href="#">ADDRESS</a>
        <a href="#">CONTACT NUMBER</a>
        <a href="login.ejs">LOGIN</a>
        <a href="signup.ejs">SIGN UP</a>
    </div>
</div>

<span onclick="openNav()">open</span></div> 

         

       <br/><br/><br/><br/><br/><br/><br/><br/><br/>
        <a href="/articles/new" class="btn btn-success">New Article</a>
        <% articles.forEach(article =>{ %>
            <br/><br/>
        <div class="card">
            <div class="card-body" style="background-color:chartreuse;">
                <div class="card-title">
                    <%= article.title %>
                </div>
                <div class="card-subtitle text-muted mb-2">
                    <%= article.created.toLocaleDateString() %>
                </div>
                <div class="card-text mb-2">
                    <%= article.description %>
                </div>
                <a href="/articles/<%= article.slug %>" class="btn btn-primary">Read More</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                <form action="/articles/<%= article.id %>?_method=DELETE" method="POST" class="d-inline">
                    <button class="btn btn-danger" type="submit">Delete </button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                </form>
                <a href="articles/edit/<%= article.id %>" class="btn btn-primary"> Edit</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                <button class="btn btn-primary" style="height:40px; width:60px" ><a href="#" onclick="like()">&#128077;</a></button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                <button class="btn btn-danger" style="height:40px; width:60px" onclick="dislike()">&#128078;</button>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
               <button class="btn btn-primary" style="height:40px; width:60px">&#128488;</button>
            </div>
          
        </div>
        <%}) %>
        
    </div>

